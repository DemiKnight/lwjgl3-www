    "point-of-view": "5.2.0",
    "pug": "3.0.2"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "3.58.0",
    "@babel/core": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "github:pmmmwh/react-refresh-webpack-plugin",
    "@prettier/plugin-pug": "1.20.1",
    "@react-aria/button": "3.4.3",
    "@react-aria/dialog": "3.1.8",
    "@react-aria/focus": "3.5.4",
    "@react-aria/interactions": "3.8.3",
    "@react-aria/link": "3.2.4",
    "@react-aria/overlays": "3.8.1",
