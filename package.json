    "@types/react-dom": "17.0.14",
    "@types/react-is": "17.0.3",
    "@types/scheduler": "0.16.2",
    "@types/web": "0.0.61",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "@typescript/lib-dom": "npm:@types/web",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.4",
    "chalk": "5.0.1",
    "cli-table": "0.3.11",
    "core-js": "3.21.1",
    "cssnano": "5.1.7",
    "eslint": "8.12.0",
    "eslint-plugin-react-hooks": "0.0.0-experimental-af730436c-20220405",
    "framer-motion": "6.2.9",
    "gzip-size": "7.0.0",
    "history": "5.3.0",
    "immer": "9.0.12",
    "jszip": "3.9.1",
    "lodash-es": "4.17.21",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.12",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.4.3",
    "postcss-scss": "4.0.3",
    "prettier": "2.6.2",
    "react": "0.0.0-experimental-af730436c-20220405",
    "react-dom": "0.0.0-experimental-af730436c-20220405",
    "react-error-boundary": "3.1.4",
    "react-fast-compare": "3.2.0",
    "react-is": "0.0.0-experimental-af730436c-20220405",
    "react-refresh": "0.0.0-experimental-af730436c-20220405",
    "reselect": "4.1.5",
    "scroll-into-view-if-needed": "2.2.29",
    "shiki": "0.10.1",
    "source-map-loader": "3.0.1",
    "typescript": "4.6.3",
    "use-context-selector": "1.3.9",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1"
  },
  "scripts": {
    "start": "run-p dev:js dev:server",
    "build:styles": "NODE_ENV=development node ./scripts/build-styles.mjs",
    "dev:js": "WDS=1 webpack serve",
    "dev:server": "NODE_ENV=development AWS_PROFILE=lwjgl node server/index.mjs",
    "dev:debug": "NODE_ENV=development AWS_PROFILE=lwjgl node --inspect --preserve-symlinks server/index.mjs",
    "dev:watch": "NODE_ENV=development AWS_PROFILE=lwjgl npx nodemon server/index.mjs",
    "dev:test-production": "NODE_ENV=production AWS_PROFILE=lwjgl node --max_old_space_size=384 server/index.mjs --test --s3proxy --nocache --pretty",
    "production": "run-p production:*",
    "production:styles": "NODE_ENV=production node ./scripts/build-styles.mjs",
    "production:js": "NODE_ENV=production node ./scripts/build-production.mjs",
    "release": "run-s production release:post",
    "release:post": "node --no-warnings ./scripts/post-production.mjs",
    "flush": "rm -r ./node_modules/.cache",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "prettier": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,mjs,md,css,pug}\"",
    "ncu": "npx npm-check-updates -i --configFileName ncu.json",
    "ncut": "npx npm-check-updates -t greatest --configFileName ncu.json",
    "ncun": "npx npm-check-updates -i -t newest --configFileName ncu-newest.json"
  }
}
