    "fastify-static": "4.0.1",
    "point-of-view": "4.13.0",
    "pug": "3.0.0"
  },
  "devDependencies": {
    "@adobe/leonardo-contrast-colors": "1.0.0-alpha.8",
    "@aws-sdk/client-cloudfront": "3.6.1",
    "@babel/core": "7.13.1",
    "@babel/preset-env": "7.13.5",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.0-beta.0",
    "@prettier/plugin-pug": "1.13.4",
    "@react-aria/button": "3.3.1",
    "@react-aria/dialog": "3.1.2",
    "@react-aria/focus": "3.2.3",
    "@react-aria/interactions": "3.3.3",
    "@react-aria/link": "3.1.2",
    "@react-aria/overlays": "3.6.1",
    "@react-aria/utils": "3.6.0",
    "@react-spring/web": "9.0.0-rc.3",
    "@stitches/react": "0.1.0-canary.12",
    "@types/lodash-es": "4.17.4",
    "@types/react-dom": "17.0.1",
    "@types/react-is": "17.0.0",
    "@types/scheduler": "0.16.1",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "autoprefixer": "10.2.4",
    "babel-loader": "8.2.2",
    "babel-plugin-polyfill-corejs3": "0.1.4",
    "chalk": "4.1.0",
    "chroma-js": "2.1.0",
    "cli-table": "0.3.5",
    "concurrently": "6.0.0",
    "core-js": "3.9.0",
    "css-loader": "5.0.2",
    "cssnano": "5.0.0-rc.0",
    "eslint": "7.20.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gzip-size": "6.0.0",
    "history": "5.0.0",
    "immer": "8.0.1",
    "jszip": "3.6.0",
    "lodash-es": "4.17.21",
    "postcss": "8.2.6",
    "postcss-import": "14.0.0",
    "postcss-preset-env": "6.7.0",
    "postcss-scss": "3.0.4",
    "prettier": "2.2.1",
    "react": "0.0.0-experimental-8af27aeed",
    "react-dom": "0.0.0-experimental-8af27aeed",
    "react-fast-compare": "3.2.0",
    "react-is": "0.0.0-experimental-8af27aeed",
    "react-refresh": "0.9.0",
    "react-router-dom": "0.0.0-experimental-ffd8c7d0",
    "react-use-gesture": "9.0.4",
    "reselect": "4.0.0",
    "rimraf": "3.0.2",
    "scroll-into-view-if-needed": "2.2.26",
    "shiki": "0.9.2",
    "style-loader": "2.0.0",
    "tiny-async-pool": "1.2.0",
    "typescript": "4.2.1-rc",
    "use-context-selector": "1.3.7",
    "webpack": "5.24.0",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "4.0.0-beta.0"
  },
  "scripts": {
    "styles:dev": "NODE_ENV=development node ./scripts/build-styles.mjs",
    "styles:prod": "NODE_ENV=production node ./scripts/build-styles.mjs",
    "bundler:dev": "WDS=1 webpack serve",
    "bundler:vendor": "NODE_ENV=development node ./scripts/vendor.mjs",
    "bundler:prod": "rimraf ./public/js/*.js && NODE_ENV=production node ./scripts/build-production.mjs",
    "bundler:post": "node ./scripts/post-production.mjs",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "server:dev": "NODE_ENV=development node server/index.mjs",
    "server:debug": "NODE_ENV=development node --inspect --preserve-symlinks server/index.mjs",
    "server:watch": "NODE_ENV=development npx nodemon server/index.mjs",
    "server:test": "NODE_ENV=production node server/index.mjs --test --s3proxy --nocache --pretty",
    "start": "concurrently \"npm run bundler:dev\" \"npm run server:watch\"",
    "release": "npm run styles:prod && npm run bundler:prod && npm run bundler:post",
    "prettier": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,mjs,md,css,pug}\"",
    "ncu": "npx npm-check-updates -i -x react,react-dom,react-is,react-router-dom",
    "ncut": "npx npm-check-updates -t greatest -x react,react-dom,react-is,react-router-dom",
    "ncun": "npx npm-check-updates -t newest"
  }
}
