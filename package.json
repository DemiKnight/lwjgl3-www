{
  "name": "lwjgl.org",
  "version": "1.1.0",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "The website of LWJGL",
  "author": "Apostolos Tsakpinis",
  "repository": {
    "type": "git",
    "url": "https://github.com/LWJGL/lwjgl3.git"
  },
  "bugs": {
    "url": "https://github.com/LWJGL/lwjgl3-www/issues"
  },
  "homepage": "https://github.com/LWJGL/lwjgl3-www#readme",
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true
  },
  "browserslist": [
    "defaults",
    "chrome >= 40",
    "firefox >= 34",
    "safari >= 8",
    "edge >= 12",
    "ios >= 9",
    "ie >= 11"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "dependencies": {
    "aws-sdk": "2.277.1",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "compression": "1.7.3",
    "express": "5.0.0-alpha.6",
    "helmet": "3.12.1",
    "pug": "2.0.3",
    "request": "2.87.0",
    "request-promise-native": "1.0.5",
    "serve-favicon": "2.5.0",
    "yargs": "12.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.54",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.54",
    "@babel/polyfill": "7.0.0-beta.54",
    "@babel/preset-env": "7.0.0-beta.54",
    "@babel/preset-flow": "7.0.0-beta.54",
    "@babel/preset-react": "7.0.0-beta.54",
    "@babel/preset-stage-1": "7.0.0-beta.54",
    "@babel/preset-stage-2": "7.0.0-beta.54",
    "@babel/preset-stage-3": "7.0.0-beta.54",
    "@emotion/core": "0.11.1",
    "@emotion/css": "0.9.5",
    "@emotion/global": "0.7.0",
    "@emotion/jsx": "0.7.0",
    "@emotion/keyframes": "0.9.1",
    "@emotion/style": "0.8.0",
    "@reach/router": "1.1.1",
    "async-sema": "2.1.1",
    "autoprefixer": "9.0.0",
    "babel-loader": "8.0.0-beta.4",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "benchmark": "2.1.4",
    "bootstrap": "4.1.2",
    "cache-loader": "1.2.2",
    "cli-table": "0.3.1",
    "core-js": "2.5.7",
    "create-subscription": "16.4.1",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "cssnano": "4.0.3",
    "exports-loader": "0.7.0",
    "fast-async": "7.0.6",
    "fbjs": "0.9.0-alpha.2",
    "fg-loadcss": "2.0.1",
    "fg-loadjs": "1.0.0",
    "focus-trap": "2.4.6",
    "gzip-size": "5.0.0",
    "http-proxy-middleware": "0.18.0",
    "immer": "1.3.1",
    "jszip": "3.1.5",
    "lodash-es": "4.17.10",
    "memoize-state": "1.5.0",
    "node-sass": "4.9.2",
    "nprogress": "0.2.0",
    "postcss-loader": "2.1.6",
    "prettier": "1.13.7",
    "query-string": "6.1.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-hot-loader": "4.3.3",
    "react-perf-devtool": "3.1.3-beta.4",
    "react-toastify": "4.1.0",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "sass-loader": "7.0.3",
    "scroll-into-view-if-needed": "2.2.14",
    "style-loader": "0.21.0",
    "terser": "3.8.0",
    "thread-loader": "1.1.5",
    "three": "0.94.0",
    "typescript-styled-plugin": "0.10.0",
    "webpack": "4.16.1",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.3",
    "whatwg-fetch": "2.0.4"
  },
  "resolutions": {
    "upath": "1.1.0"
  },
  "scripts": {
    "start": "node server",
    "test-production": "cross-env NODE_ENV=production node server --test --s3proxy --nocache --pretty",
    "run-production": "cross-env NODE_ENV=production node server --s3proxy",
    "watch": "nodemon --watch server server/index.js",
    "styles": "node ./scripts/build-styles.js",
    "vendor": "webpack --config webpack.vendor.config.js --bail --progress",
    "bundle-analyzer-dev": "cross-env NODE_ENV=development webpack --progress --json > webpack-dev.manifest.json && webpack-bundle-analyzer webpack-dev.manifest.json",
    "bundle-analyzer-prod": "cross-env NODE_ENV=production webpack --progress --json > webpack-prod.manifest.json && webpack-bundle-analyzer webpack-prod.manifest.json",
    "bundle-verbose": "cross-env NODE_ENV=production webpack --progress --display-error-details --verbose",
    "production": "cross-env NODE_ENV=production webpack --bail --progress --json > ./public/js/webpack.manifest.json",
    "post-production": "node ./scripts/post-production.js",
    "release-js": "yarn run production && yarn run post-production",
    "release": "yarn run styles && yarn run production && yarn run post-production",
    "deploy": "node ./scripts/deploy.js",
    "format": "prettier --write \"{client,server,scripts}/**/*.js*\"",
    "format-styles": "prettier --write \"client/styles/**/*.scss*\""
  }
}
