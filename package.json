    "@types/web": "0.0.58",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "@typescript/lib-dom": "npm:@types/web",
    "autoprefixer": "10.4.4",
    "babel-loader": "8.2.3",
    "chalk": "5.0.1",
    "cli-table": "0.3.11",
    "concurrently": "7.0.0",
    "core-js": "3.21.1",
    "css-loader": "6.7.1",
    "cssnano": "5.1.4",
    "eslint": "8.11.0",
    "eslint-plugin-react-hooks": "0.0.0-experimental-645ec5d6f-20220317",
    "framer-motion": "6.2.8",
    "gzip-size": "7.0.0",
    "history": "5.3.0",
    "immer": "9.0.12",
    "jszip": "3.7.1",
    "lodash-es": "4.17.21",
    "postcss": "8.4.12",
    "postcss-import": "14.0.2",
    "postcss-preset-env": "7.4.3",
    "postcss-scss": "4.0.3",
    "prettier": "2.6.0",
    "react": "0.0.0-experimental-645ec5d6f-20220317",
    "react-dom": "0.0.0-experimental-645ec5d6f-20220317",
    "react-error-boundary": "3.1.4",
    "react-fast-compare": "3.2.0",
    "react-is": "0.0.0-experimental-645ec5d6f-20220317",
    "react-refresh": "0.0.0-experimental-fe905f152-20220107",
    "reselect": "4.1.5",
    "rimraf": "3.0.2",
    "scroll-into-view-if-needed": "2.2.29",
    "shiki": "0.10.1",
    "source-map-loader": "3.0.1",
    "style-loader": "3.3.1",
    "typescript": "4.6.2",
    "use-context-selector": "1.3.9",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "scripts": {
    "styles:dev": "NODE_ENV=development node ./scripts/build-styles.mjs",
    "styles:prod": "NODE_ENV=production node ./scripts/build-styles.mjs",
    "bundler:dev": "WDS=1 webpack serve",
    "bundler:vendor": "NODE_ENV=development node ./scripts/vendor.mjs",
    "bundler:prod": "rimraf ./public/js/*.js && NODE_ENV=production node ./scripts/build-production.mjs",
    "bundler:post": "node ./scripts/post-production.mjs",
    "flush": "rimraf ./node_modules/.cache",
    "lint": "eslint --ext .ts,.js,.tsx client/",
    "server:dev": "NODE_ENV=development AWS_PROFILE=lwjgl node server/index.mjs",
    "server:debug": "NODE_ENV=development AWS_PROFILE=lwjgl node --inspect --preserve-symlinks server/index.mjs",
    "server:watch": "NODE_ENV=development AWS_PROFILE=lwjgl npx nodemon server/index.mjs",
    "server:test": "NODE_ENV=production AWS_PROFILE=lwjgl node --max_old_space_size=384 server/index.mjs --test --s3proxy --nocache --pretty",
    "start": "concurrently \"npm run bundler:dev\" \"npm run server:dev\"",
    "release": "npm run styles:prod && npm run bundler:prod && npm run bundler:post",
    "prettier": "prettier --write \"{client,server,scripts}/**/*.{ts,tsx,js,mjs,md,css,pug}\"",
    "ncu": "npx npm-check-updates -i --configFileName ncu.json",
    "ncut": "npx npm-check-updates -t greatest --configFileName ncu.json",
    "ncun": "npx npm-check-updates -i -t newest --configFileName ncu-newest.json"
  }
}
